<html
  xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
  lang="en"
  metal:use-macro="here/prefs_main_template/macros/master"
  i18n:domain="eea.google">

<body>
<div metal:fill-slot="prefs_configlet_main" tal:define="
  table python:view.table">
  <h1 class="documentFirstHeading"
    tal:content="here/title_or_id">Google Analytics Report</h1>

  <a href=""
    class="link-parent"
    tal:attributes="href string:../"
    i18n:translate="label_up_to_plone_setup">Up to Site Setup</a>

  <!-- Reports -->
  <table class="listing nosort controlpanel-listing">
    <thead>
    <tr>
      <th tal:content="here/dimensions">Dimensions</th>
      <th tal:content="here/metrics">Metrics</th>
    </tr>
  </thead>
  <tbody>
    <tal:row tal:repeat="row table">
      <tr tal:define="oddrow repeat/row/odd"
          tal:attributes="class python:oddrow and 'even' or 'odd'">
        <td tal:define="dimensions python:row[0]">
          <span tal:content="python:dimensions.get(here.dimensions, '(not found)')" />
        </td>
        <td tal:define="metrics python:row[1]" style="text-align: center">
          <span tal:content="python:metrics.get(here.metrics, 0)" />
        </td>
      </tr>
    </tal:row>
  </tbody>
  </table>

</div>
</body>
</html>
